[Unit]
Description=Probemon MQTT Bridge Service

[Service]
ExecStartPre=/usr/local/bin/probemon/prereq.sh
ExecStart=/usr/local/bin/probemon/probemon -i wlan1 --mac-info --ssid -x 192.168.0.18 -m "/arda/palantir/devices/general.interfaces.probemon/readings/probe"
StandardOutput=null

[Install]
WantedBy=multi-user.target
Alias=probemon.service